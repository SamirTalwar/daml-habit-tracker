module Tests where

import DA.Date

import Main

recordHabits = scenario do
  alice <- getParty "Alice"

  run <- submit alice do
    create Habit with
      owner = alice
      name = "Run"

  submit alice do
    exercise run Record with
      dateCompleted = date 2020 Jul 1
  submit alice do
    exercise run Record with
      dateCompleted = date 2020 Jul 2
  submit alice do
    exercise run Record with
      dateCompleted = date 2020 Jul 3
